@model KontoBankowe

@{
    ViewBag.Title = "Edycja konta bankowego";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = String.Format("Edycja konta bankowego"), SubTitle = "Edytuj konto bankowe." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Konto bankowe</legend>

        @Html.HiddenFor(model => model.Id)

        @Html.Partial("_KontoBankowe", Model)

        <p>
            <input type="submit" value="Zapisz" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
