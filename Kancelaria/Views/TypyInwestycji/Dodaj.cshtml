@model TypInwestycji

@{
    ViewBag.Title = "Dodawanie nowego typu inwestycji";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = "Dodawanie typu inwestycji.", SubTitle = "Dodaj nowy typ inwestycji." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Typ inwestycji</legend>

        @Html.Partial("_TypInwestycji", Model)
        <hr />
        <p>
            <input type="submit" value="Dodaj typ inwestycji" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
