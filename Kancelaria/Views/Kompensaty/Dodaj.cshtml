@model Kompensata

@{
    ViewBag.Title = "Dodawanie nowej kompensaty";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = "Dodawanie kompensaty.", SubTitle = "Dodaj nową kompensatę." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>kompensata</legend>

        @Html.Partial("_Kompensata", Model)
        <hr />
        <p>
            <input type="submit" value="Dodaj kompensatę" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
