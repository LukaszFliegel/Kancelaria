@model RokObrotowy

@{
    ViewBag.Title = "Dodawanie nowego roku obrotowego";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = "Dodawanie roku obrotowego.", SubTitle = "Dodaj nowy rok obrotowy." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Rok obrotowy</legend>

        @Html.Partial("_RokObrotowy", Model)
        <hr />
        <p>
            <input type="submit" value="Dodaj rok obrotowy" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
