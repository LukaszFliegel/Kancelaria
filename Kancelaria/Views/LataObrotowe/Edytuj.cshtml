@model RokObrotowy

@{
    ViewBag.Title = "Edycja roku obrotowego";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = String.Format("Edycja roku obrotowego"), SubTitle = "Edytuj rok obrotowy." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Rok obrotowy</legend>

        @Html.HiddenFor(model => model.Id)

        @Html.Partial("_RokObrotowy", Model)

        <p>
            <input type="submit" value="Zapisz" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
