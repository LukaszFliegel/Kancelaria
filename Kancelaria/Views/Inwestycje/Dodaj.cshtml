@model Inwestycja

@{
    ViewBag.Title = "Dodawanie nowej inwestycji";
}

@section featured {
    @Html.Partial("_FeaturedSection", new FeaturedSectionModel() { Title = "Dodawanie inwestycji.", SubTitle = "Dodaj nową inwestycję." })
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Inwestycja</legend>

        @Html.Partial("_Inwestycja", Model)
        <hr />
        <p>
            <input type="submit" value="Dodaj inwestycję" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Powrót do kartoteki", "Kartoteka")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
